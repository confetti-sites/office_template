{
  environments: [
    {
      key: 'localhost',
      containers: [
        // Main view service
        {
          name: 'confetti-cms/view-php',
          hosts: [
            'office_dev.confetti-cms.localhost'
          ],
          paths: [
            '/',
          ],
        },
        // For all other services
        {
          hosts: [
            'office_dev.confetti-cms.localhost'
          ],
          paths: [
            '/conf_api',
          ],
          user_service_in_uri: true,
        },
      ],
    },
    {
      key: 'production',
      containers: [
        // Main view service
        {
          name: 'confetti-cms/view-php',
          hosts: [
            'office_dev.confetti-cms.localhost'
          ],
          paths: [
            '/',
          ],
        },
        // For all other services
        {
          hosts: [
            'office_dev.confetti-cms.localhost'
          ],
          paths: [
            '/conf_api',
          ],
          user_service_in_uri: true,
        },
      ],
    },
  ],
}
